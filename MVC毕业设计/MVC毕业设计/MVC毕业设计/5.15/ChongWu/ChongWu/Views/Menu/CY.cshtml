
@{
    ViewBag.Title = "CY";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>CY</h2>

@section di{
    
    <main class="main-content">
        <!--== Start Page Header Area Wrapper ==-->
        <div class="page-header-area" data-bg-img="/assets/img/photos/bg1.png">
            <div class="container pt--0 pb--0">
                <div class="row">
                    <div class="col-12">
                        <div class="page-header-content">
                            <h2 class="title">登录</h2>
                            <nav class="breadcrumb-area">
                                <ul class="breadcrumb">
                                    <li><a href="index.html">首页</a></li>
                                    <li class="breadcrumb-sep">//</li>
                                    <li>登录</li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--== End Page Header Area Wrapper ==-->
        <!--== Start My Account Area Wrapper ==-->
        <section class="account-area">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="login-form-content">
                            <form  method="post" action="/Menu/CY">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="form-group">
                                            <label for="username">用户名 <span class="required">*</span></label>
                                            <input id="username" class="form-control" type="text" name="loginname">
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <div class="form-group">
                                            <label for="password">密码 <span class="required">*</span></label>
                                            <input id="password" class="form-control" type="password" name="loginpassword">
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <div class="form-group">
                                            <input id="Button1" class="btn-login" href="#" type="submit" value="登录" />
                                            
                                        </div>
                                      
                                    </div>
                                    <div class="col-12">
                                        <div class="form-group account-info-group mb--0">
                                            <div class="rememberme-account">
                                                <div class="form-check">
                                                    <input class="form-check-input" type="checkbox" value="复选框" id="defaultCheck1" name="chec" >
                                                    <label class="form-check-label" for="defaultCheck1">我已阅读<input id="Button1" type="button" value="用户协议" class="btn btn-link" />并同意</label>
                                                </div>
                                            </div>
                                            <a class="lost-password" href="account-register.html">忘记密码?</a>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--== End My Account Area Wrapper ==-->
        <!--== Start Feature Area Wrapper ==-->
        <div class="feature-area">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3">
                        <div class="feature-icon-box">
                            <div class="icon-box">
                                <img class="icon-img" src="~/static/picture/f1.png" width="46" height="34" alt="Icon-HasTech">
                            </div>
                            <div class="content">
                                <h5 class="title">Free Shipping</h5>
                                <p>Capped at $39 per order</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <div class="feature-icon-box">
                            <div class="icon-box">
                                <img class="icon-img" src="~/static/picture/f2.png" width="43" height="34" alt="Icon-HasTech">
                            </div>
                            <div class="content">
                                <h5 class="title">Card Payments</h5>
                                <p>12 Months Installments</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <div class="feature-icon-box">
                            <div class="icon-box">
                                <img class="icon-img" src="~/static/picture/f3.png" width="39" height="39" alt="Icon-HasTech">
                            </div>
                            <div class="content">
                                <h5 class="title">Easy Returns</h5>
                                <p>Shop With Confidence</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3">
                        <div class="feature-icon-box">
                            <div class="icon-box">
                                <img class="icon-img" src="~/static/picture/f4.png" width="36" height="39" alt="Icon-HasTech">
                            </div>
                            <div class="content">
                                <h5 class="title">24/7 Support</h5>
                                <p>Contact 24 hours everyday</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--== End Feature Area Wrapper ==-->
    </main>
    
    
    }